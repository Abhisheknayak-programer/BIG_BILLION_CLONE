    <script>
        let visible = document.getElementById("continue");

        function storyFunc(){
            let Twitter_post = `https://twitter.com/intent/tweet?text=https://big-billion-spin.herokuapp.com/`;
            window.open(Twitter_post);
            
            visible.style.display = ("block");
            
            setTimeout(() => {
            swal({
            title: "Good job!",
            text: "You clicked Shared Sucessfully",
            icon: "success",
            button: "Click Continue",
            });
            }, 3000);
        }


        function visibled(){
            visible.style.display = ("block");
                       
            setTimeout(() => {
            swal({
            title: "Good job!",
            text: "You clicked Shared Sucessfully",
            icon: "success",
            button: "Click Continue",
            });
            }, 3000);
           
        }



        function method2(){
